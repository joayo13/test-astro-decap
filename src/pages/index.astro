---
import "@assets/styles/global.css";
import PageLayout from "@layouts/PageLayout.astro";
import { getEntry } from 'astro:content';
const entry = await getEntry('index', 'homepage'); // Match the filename
const data = entry?.data; // Extract the frontmatter data
---

<PageLayout>
	<div class="flex flex-col text-blue-600">
		<a href="/admin">CMS</a>
		<a href="/admin/config.yml">CMS Config</a>
		<a href="/blog/seed">Seed Post</a>
		<h1>{data?.title}</h1>
<p>{data?.paragraph}</p>
	</div>
</PageLayout>
